    margin: 0 10px;
    padding: 5px 10px;
}

.wizard .foot button {
    margin: 0 4px;
    /* background-color: #0072bb; */
}

.wizard .foot button:hover {
    /* background-color: #34abff; */
}

.wizard .foot .extra-area {
    float: left
}

.wizard .foot .extra-area a {
