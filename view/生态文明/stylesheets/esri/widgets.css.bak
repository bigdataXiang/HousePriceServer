/* basepopup */
.basepopup {
    position: absolute;
    width: 1000px;
    opacity: 0.9;
    z-index: 9999;
}

.basepopup .popup-con {
    background-color: #fff;
    position: absolute;
    line-height: normal;
    box-shadow: 1px 1px 10px #827F7F;
}

.basepopup .title-con {
    padding: 5px 10px;
    font-weight: bold;
    border-bottom: solid #ccc 1px;
}

.basepopup .closebtn {
    position: absolute;
    display: inline-block;
    top: 0;
    right: -30px;
    width: 30px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    cursor: pointer;
    background-color: rgba(173, 170, 170, 0.6);
}

.basepopup .closebtn i {
    color: #fff;
}

.basepopup .closebtn:hover i {
    color: rgb(32,147,239);
}

.basepopup .tailcon {
    position: absolute;
    left: 0px;
    bottom: -20px;
}

.basepopup .tailcon.t, .basepopup .tailcon.b {
    width: 20px;
    height: 10px;
}

.basepopup .tailcon.l, .basepopup .tailcon.r {
    width: 10px;
    height: 20px;
}

.basepopup .tailcon > div {
    width: 0;
    height: 0;
    display: inline-block;
    vertical-align: top;
}

.basepopup .tailcon.t > div {
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-bottom: 10px solid #fff;
}

.basepopup .tailcon.b > div {
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-top: 10px solid #fff;
}

.basepopup .tailcon.l > div {
    border-top: 10px solid transparent;
    border-bottom: 10px solid transparent;
    border-right: 10px solid #fff;
}

.basepopup .tailcon.r > div {
    border-top: 10px solid transparent;
    border-bottom: 10px solid transparent;
    border-left: 10px solid #fff;
}

.basepopup .page-con {
    text-align: right;
    padding: 5px 10px 5px 0;
}

.basepopup .page-con a {
    text-align: right;
    padding: 0 3px;
}

.breakslider {
    position: relative;
}

