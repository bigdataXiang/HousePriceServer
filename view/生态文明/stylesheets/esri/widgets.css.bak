/*drawtools*/
.drawtools {
    padding: 5px 10px;
}

.drawtools li {
    float: left;
    list-style: none;
    padding: 1px 5px;
    border: solid transparent 1px;
    cursor: pointer;
}

.drawtools li.active {
    border: solid #F3550B 1px;
}

.drawtools li.close-item {
    display: none;
}

/*dropdown*/
.dropdown {
    position: relative;
}

.dropdown .choices-con {
    width: 200px;
    zoom: 1;
}

.dropdown ul.choices {
    overflow: hidden;
    height: auto;
    border: 1px solid #aaa;
    background-color: #fff;
    background-image: -webkit-linear-gradient(top, #eeeeee 1%, #ffffff 15%);
    background-image: -moz-linear-gradient(top, #eeeeee 1%, #ffffff 15%);
    background-image: linear-gradient(top, #eeeeee 1%, #ffffff 15%);
}

.dropdown ul.choices li {
    float: left;
    list-style: none;
}

.dropdown ul.choices li.search-choice {
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    -webkit-background-clip: padding-box;
    /*-moz-background-clip: padding-box;*/
    background-image: -webkit-linear-gradient(top, #f4f4f4 20%, #f0f0f0 50%, #e8e8e8 52%, #eeeeee 100%);
    background-image: -moz-linear-gradient(top, #f4f4f4 20%, #f0f0f0 50%, #e8e8e8 52%, #eeeeee 100%);
    background-image: linear-gradient(top, #f4f4f4 20%, #f0f0f0 50%, #e8e8e8 52%, #eeeeee 100%);
    -webkit-box-shadow: 0 0 2px #ffffff inset, 0 1px 0 rgba(0, 0, 0, 0.05);
    -moz-box-shadow: 0 0 2px #ffffff inset, 0 1px 0 rgba(0, 0, 0, 0.05);
    box-shadow: 0 0 2px #ffffff inset, 0 1px 0 rgba(0, 0, 0, 0.05);
    background-clip: padding-box;
    background-color: #e4e4e4;
    color: #333;
    border: 1px solid #aaaaaa;
    line-height: 13px;
    padding: 3px 4px 3px 5px;
    margin: 3px 0 3px 5px;
    list-style: none;
    cursor: default;
}

.dropdown ul.choices li.search-choice span {
    color: #333;
    line-height: 13px;
    padding-right: 4px;
    cursor: default;
}

.dropdown ul.choices li.search-field input {
    border: 0;
    outline: 0;
    width: 180px;
    margin-top: 4px;
    padding: 2px 6px;
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    color: #000;
}

.dropdown .dropdown-menu {
    display: none;
    min-width: 200px;
    max-height: 250px;
    overflow-y: auto;
    margin: 0;
}

.dropdown .dropdown-menu a:hover {
    cursor: pointer;
}

/*extradropdown*/
.extradropdown {
    position: relative;
}

.extradropdown .dropdown-menu {
    display: none;
    min-width: 200px;
    max-height: 250px;
    overflow-y: auto;
    margin: 0;
}

.extradropdown .dropdown-menu li:hover {
    cursor: pointer;
}

.extradropdown .dropdown-menu li.selected a {
    /*background: url(../gistech/widgets/images/accept.png) no-repeat right center;*/
}

/* color picker */
.colorpicker {
    width: 24px;
    height: 24px;
    border: 1px solid #ddd;
    border-radius: 4px;
    cursor: pointer;
}

.colorpicker:hover {
    box-shadow: 0px 0px 5px #00acec;
    border: none;
}

.colorpicker.transparent {
    background: url(../gistech_1010/widgets/images/transparent.png) 0 0 no-repeat;
}

/* color popup */
.colorpop.popover,
.colorbrewer.popover {
    margin-top: 0px;
    width: 200px;
    z-index: 30000;
}

.colorpop.popover > .popover-title,
.colorbrewer.popover > .popover-title {
    text-align: right;
    padding: 3px 10px;
}

.colorpop.popover button.close,
.colorbrewer.popover button.close {
    margin-left: 95px;
    margin-top: 1px;
}

.colorpop.popover ul.colorlist > li {
    list-style-type: none;
    display: inline-block;
    width: 18px;
    height: 18px;
    margin: 3px;
    box-shadow: 0px 0px 5px #ccc;
}

.colorpop.popover ul.colorlist > li:hover,
.colorbrewer.popover ul.colorlist > li:hover {
    box-shadow: 0px 0px 5px #00acec;
    cursor: pointer;
}

.colorpop.popover ul.colorlist > li.active,
.colorbrewer.popover ul.colorlist > li.active {
    box-shadow: 0px 0px 5px #00acec;
}

.colorpop.popover ul.colorlist > li.transparent {
    background: url(../gistech/widgets/images/transparent.png) -3px -2px no-repeat;
}

/* colorbrewer */
.colorbrewer.popover ul.colorlist {
    height: 174px;
    max-height: 174px;
}

.colorbrewer.popover ul.colorlist > li {
    list-style-type: none;
    width: 140px;
    height: 18px;
    margin: 3px;
}

.colorbrewer.popover ul.colorlist > li > div {
    float: left;
    width: 20px;
    height: 18px;
}

/*pager*/
.pagination {
    margin: 0;
}

.pagination a {
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
}

.pagination > li > a,
.pagination > li > span {
    /*color: rgb(32,147,239);*/
    color: #2093ef;
}

.pagination > .active > a,
.pagination > .active > span,
.pagination > .active > a:hover,
.pagination > .active > span:hover,
.pagination > .active > a:focus,
.pagination > .active > span:focus {
    /*background-color: rgb(32,147,239);*/
    /*border-color: rgb(32,147,239);*/
    background-color: #2093ef;
    border-color: #2093ef;
    /*background-color: rgb(75, 167, 240);*/
    /*border-color: rgb(239, 239, 239);*/
}

.pagination > li > a:hover,
.pagination > li > span:hover,
.pagination > li > a:focus,
.pagination > li > span:focus {
    color: #2093ef;
}

