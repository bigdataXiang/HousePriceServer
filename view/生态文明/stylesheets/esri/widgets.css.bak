.scroll {
    overflow-x: auto;
    overflow-y: auto;
}

.noscroll {
    overflow: hidden;
}

.scroll-x {
    overflow-x: auto;
    overflow-y: hidden;
}

.scroll-y {
    overflow-x: hidden;
    overflow-y: auto;
}

.shadow {
    box-shadow: 0px 0px 2px #595959;
}

.light-shadow {
    box-shadow: 0px 0px 1px #595959;
}

.blue {
    color: #00acec;
}

.blue-bg {
    background-color: #00acec;
}

.green {
    color: #7dc67d;
}

.green-bg {
    background-color: #7dc67d;
}

.red {
    color: #ff714d;
}

.red-bg {
    background-color: #ff714d;
}

.purple {
    color: rgb(32,147,239);
}

.purple-bg {
    background-color: rgb(32,147,239);
}

/* tip */
.tip {
    position: absolute;
    z-index: 20010;
    opacity: 0;
}

.tip > div,
.tippet > div {
    float: left;
}

.tip > .tip-text,
.tippet > .tip-text {
    overflow: hidden;
    word-wrap: break-word;
    max-width: 255px;
    padding: 4px 10px;
}

/* dialog */
.dialog {
    position: absolute;
    z-index: 20001;
    margin: 0;
    overflow: hidden;
    opacity: 0;
}

.dialog.autocenter {
    width: 100%;
    height: 100%;
}

.dialog > *:first-child {
    z-index: 20002;
}

.dialog.autocenter > *:first-child {
    position: absolute;
}

.dialog > .shelter {
    display: none;
    width: 100%;
    height: 100%;
    background-color: #000;
    opacity: 0.45;
}

.dialog.autocenter > .shelter {
    display: block;
}

.dialog > .box {
    background-color: #f5f5f5;
    border: 1px solid #ddd;
    border-radius: 4px;
    box-shadow: 0px 0px 5px #ccc;
}

.dialog > div > .text {
    width: 100%;
    padding: 5%;
    background-color: #fff;
    border-bottom: 1px solid #ddd;
}

.dialog > div > .buttons {
    text-align: right;
    margin: 10px 10px 10px auto;
}

/* alert */
.dialog > .alerter {
    width: 320px;
}

.alerter .checkbox {
    padding-left: 30px;
}

/* confirm & prompt */
.dialog > .confirm,
.dialog > .prompt {
    width: 360px;
}

.confirm .checkbox {
    padding-left: 30px;
}

/* loading */
.dialog > .loading {
    color: #fff;
}

/* image box */
.dialog > .imagebox {
    border: none;
    border-radius: 0;
    background-repeat: no-repeat;
}

.dialog .imagebox > .buttons {
    opacity: 0;
}

.imagebox .opening {
    float: left;
    margin-left: 10px;
    margin-top: 10px;
}

.imagebox a:hover {
    text-decoration: none;
}

