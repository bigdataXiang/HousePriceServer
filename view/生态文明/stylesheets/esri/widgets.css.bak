/* override bootstrap */
table th {
    background-color: #f0f0f0;
}

.table-bordered > thead > tr > th, .table-bordered > thead > tr > td {
    border-bottom-width: 1px;
}

.table-bordered > thead > tr > th {
    text-align: center;
}

ul, ol {
    padding-left: 0;
    margin-bottom: 0;
}

.progress-bar {
    background-color: #2093ef;
}

.legends li img {
    display: block;
    max-width: 260px;
    margin: 4px 0 5px 10px;
}

.legends li {
    list-style-type: none;
    padding: 3px;
    border-bottom: 1px solid #d7d7d7;
}

.legends .label {
    font-size: 14px;
    font-weight: 400;
    color: #595959;
    cursor: default;
    font-size: 12px;
}

.scroll-y::-webkit-scrollbar-track {
    box-shadow: 1px 1px 5px rgba(0, 0, 0, .2) inset;
}

/* image view */
.ImageView{
    width: 100%;
    height: 100%;
}
.ImageView img{
    width: 100%;
    height: 100%;
}